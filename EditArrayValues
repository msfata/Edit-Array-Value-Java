 */
public class EditArrayValues{
	public static void main(String[] args) {
		int [] arr= {0,1,2,0,3,4};
		for (int i = 0; i < arr.length; i++) {
			if(arr[i]==0){
				arr[i]=arr[i]+5;
				
			}
		}
		for (int i = 0; i < arr.length; i++) {
			System.out.print(arr[i]+",");
		}

	}
}
